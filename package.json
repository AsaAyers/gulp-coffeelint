{
  "name": "gulp-coffeelint",
  "version": "0.0.2",
  "description": "Lint your CoffeeScript using gulp and CoffeeLint",
  "keywords": [
    "gulpplugin",
    "lint",
    "coffee",
    "coffeelint",
    "coffeescript",
    "coffee-script",
    "codeconventions"
  ],
  "repository": "janraasch/gulp-coffeelint",
  "author": {
    "name": "Jan Raasch",
    "email": "jan@janraasch.com",
    "url": "http://janraasch.com"
  },
  "files": [
    "index.js"
  ],
  "scripts": {
    "prepublish": "node ./node_modules/gulp/bin/gulp.js coffee --require coffee-script",
    "test": "node ./node_modules/mocha/bin/mocha ./test/*.coffee --require coffee-script --reporter spec && node ./node_modules/coffeelint/bin/coffeelint gulpfile.coffee index.coffee test -f ./coffeelint.json"
  },
  "dependencies": {
    "event-stream": "~3.0.20",
    "coffeelint": "~1.0.2",
    "gulp-util": "~2.2.0",
    "coffeelint-stylish": "0.0.1"
  },
  "devDependencies": {
    "mocha": "~1.16.1",
    "should": "~2.1.0",
    "gulp": "~3.2.0",
    "gulp-coffee": "~1.2.2",
    "coffee-script": "~1.6.3"
  },
  "engines": {
    "node": ">=0.9.0"
  },
  "license": "MIT"
}
